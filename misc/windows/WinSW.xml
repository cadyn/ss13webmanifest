<service>
  <id>ss13manifest</id>
  <name>SS13 Web Manifest</name>
  <description>This service runs the SS13 Web Manifest</description>
  <executable>java</executable>
  <arguments>-jar "PATHTO\build\libs\manifest-0.1.1.jar" --socket.host=SS13HOSTNAME --socket.port=SS13PORT --server.port=4583</arguments>
  <log mode="roll"></log>
</service>